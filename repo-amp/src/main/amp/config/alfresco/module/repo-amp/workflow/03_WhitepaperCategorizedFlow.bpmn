  <process id="ID_WhitepaperCategorizedFlow" name="NAME_WhitepaperCategorizedFlow" isExecutable="true">
    <startEvent id="alfrescoStartevent1" name="Alfresco start" activiti:formKey="wf:submitReviewTask"></startEvent>
    <userTask id="alfrescoUsertask1" name="Dev review of whitepaper" activiti:assignee="${bpm_assignee.properties.userName}" activiti:formKey="wf:activitiReviewTask">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('reviewOutcome', task.getVariable('wf_reviewOutcome'));
logger.warn("In dev review, set reviewOutcome to " + execution.getVariable("reviewOutcome"));
logger.warn("Wololo");]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[logger.info("Logging stuff!");
if( bpm_package.children.length <= 0 ) {
	logger.warn("no node attached to workflow");
} else {
	logger.warn("node attached to workflow: " + bpm_package.children[0]);
}]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
        <activiti:taskListener event="create" class="org.example.playground.workflow.Categorizer"></activiti:taskListener>
      </extensionElements>
    </userTask>
    <endEvent id="endevent1" name="End"></endEvent>
    <sequenceFlow id="flow1" sourceRef="alfrescoStartevent1" targetRef="alfrescoUsertask1"></sequenceFlow>
    <exclusiveGateway id="exclusivegateway1" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow3" name="Discard" sourceRef="exclusivegateway1" targetRef="endevent1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewOutcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="alfrescoUsertask2" name="Revise or discard whitepaper" activiti:assignee="${initiator.properties.userName}" activiti:formKey="wf:activitiReviewTask">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('reviewOutcome', task.getVariable('wf_reviewOutcome'));
logger.warn("In revise/discard, set reviewOutcome to " + execution.getVariable("reviewOutcome"));]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <exclusiveGateway id="exclusivegateway2" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow6" sourceRef="alfrescoUsertask1" targetRef="exclusivegateway2"></sequenceFlow>
    <sequenceFlow id="flow7" name="Reject" sourceRef="exclusivegateway2" targetRef="alfrescoUsertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewOutcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="flow8" sourceRef="alfrescoUsertask2" targetRef="exclusivegateway1"></sequenceFlow>
    <sequenceFlow id="flow9" name="Revise" sourceRef="exclusivegateway1" targetRef="alfrescoUsertask1">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="alfrescoUsertask3" name="QA review of whitepaper" activiti:assignee="${bpm_assignee.properties.userName}" activiti:formKey="wf:activitiReviewTask">
      <extensionElements>
        <activiti:taskListener event="complete" class="org.alfresco.repo.workflow.activiti.tasklistener.ScriptTaskListener">
          <activiti:field name="script">
            <activiti:string><![CDATA[execution.setVariable('reviewOutcome', task.getVariable('wf_reviewOutcome'));
logger.warn("In qa review, set reviewOutcome to " + execution.getVariable("reviewOutcome"));]]></activiti:string>
          </activiti:field>
        </activiti:taskListener>
      </extensionElements>
    </userTask>
    <sequenceFlow id="flow10" name="Approve" sourceRef="exclusivegateway2" targetRef="alfrescoUsertask3">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <exclusiveGateway id="exclusivegateway3" name="Exclusive Gateway"></exclusiveGateway>
    <sequenceFlow id="flow11" sourceRef="alfrescoUsertask3" targetRef="exclusivegateway3"></sequenceFlow>
    <sequenceFlow id="flow12" name="Reject" sourceRef="exclusivegateway3" targetRef="alfrescoUsertask2">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewOutcome == 'Reject'}]]></conditionExpression>
    </sequenceFlow>
    <userTask id="alfrescoUsertask4" name="Publish whitepaper" activiti:assignee="${initiator.properties.userName}" activiti:formKey="wf:approvedTask"></userTask>
    <sequenceFlow id="flow13" name="Approve" sourceRef="exclusivegateway3" targetRef="alfrescoUsertask4">
      <conditionExpression xsi:type="tFormalExpression"><![CDATA[${reviewOutcome == 'Approve'}]]></conditionExpression>
    </sequenceFlow>
    <endEvent id="endevent2" name="End"></endEvent>
    <sequenceFlow id="flow14" name="Live" sourceRef="alfrescoUsertask4" targetRef="endevent2"></sequenceFlow>
  </process>
